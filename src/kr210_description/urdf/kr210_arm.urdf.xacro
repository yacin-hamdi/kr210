<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

<link name="base_footprint"/>

<link name="base_link">
    <visual>
        <geometry>
            <mesh filename="package://kr210_description/meshes/kr210l150/visual/base_link.dae"/>
        </geometry>
        <origin xyz="0 0 0" rpy="-${pi/2} 0 0"/>
        <material name="">
            <color rgba="0.75294 0.75294 0.75294 1"/>
        </material>
    </visual>
</link>

<link name="link_1">
    <visual>
        <geometry>
            <mesh filename="package://kr210_description/meshes/kr210l150/visual/link_1.dae"/>
        </geometry>
        <origin xyz="0 0 0" rpy="0 0 0"/>
    </visual>
</link>

<link name="link_2">
    <visual>
        <geometry>
            <mesh filename="package://kr210_description/meshes/kr210l150/visual/link_2.dae"/>
        </geometry>
        <origin xyz="0 0 0" rpy="-${pi/2} 0 0"/>
    </visual>
</link>

<link name="link_3">
    <visual>
        <geometry>
            <mesh filename="package://kr210_description/meshes/kr210l150/visual/link_3.dae"/>
        </geometry>
        <origin xyz="0 -0.184 0" rpy="${-pi/2} 0 0"/>
    </visual>
</link>

<link name="link_4">
    <visual>
        <geometry>
            <mesh filename="package://kr210_description/meshes/kr210l150/visual/link_4.dae"/>
        </geometry>
        <origin xyz="0 0 0" rpy="0 0 0"/>
    </visual>
</link>


<joint name="fixed_base_joint" type="fixed">
    <parent link="base_footprint"/>
    <child link="base_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
</joint>

<joint name="joint_1" type="revolute">
    <parent link="base_link"/>
    <child link="link_1"/>
    <origin xyz="0 0 0.33" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="${-185.0 * (pi / 180.0)}" upper="${185.0 * (pi / 180.0)}" effort="300" velocity="${123.0 * (pi / 180.0)}"/>
</joint>

<joint name="joint_2" type="revolute">
    <parent link="link_1"/>
    <child link="link_2"/>
    <origin xyz="0.35 0 0.42" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="${-45*pi/180}" upper="${85*pi/180}" effort="300" velocity="${115*pi/180}"/>
</joint>

<joint name="joint_3" type="revolute">
    <parent link="link_2"/>
    <child link="link_3"/>
    <origin xyz="0 0 1.25" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="${-210*pi/180}" upper="${65*pi/180}" effort="300" velocity="${112 * pi/180}"/>
</joint>

<joint name="joint_4" type="revolute">
    <parent link="link_3"/>
    <child link="link_4"/>
    <origin xyz="0.96 0 -0.054" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="${-125 * pi/180}" upper="${125*pi/180}" effort="300" velocity="${179*pi/180}"/>
</joint>

</robot>
